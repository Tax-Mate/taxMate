<p id="notice"><%= notice %></p>

<h1>Federal Income Tax Calculator</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Tax</th>
      <th>Time type</th>
      <th>Income</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @federalforms.each do |federalform| %>
      <tr>
        <td><%= federalform.date %></td>
        <td><%= federalform.tax %></td>
        <td><%= federalform.time_type %></td>
        <td><%= federalform.income %></td>
        <td><%= link_to 'Show', edit_federalform_path(federalform) %></td>
        <td><%= link_to 'Destroy', federalform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create new Federal tax calculation', new_federalform_path %>
